display_errors = true
error_reporting = E_ALL & ~E_DEPRECATED & ~E_STRICT & ~E_WARNING
error_log = /var/sync/logs/error_log.txt
log_errors = On
user_ini.cache_ttl = 0
max_execution_time = 600
max_input_time = 300
max_input_vars = 2500
memory_limit = 1G
post_max_size = 256M
upload_max_filesize = 256M